{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "13",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  }
}